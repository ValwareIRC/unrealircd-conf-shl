{
  "name": "unrealircd-conf-shl",
  "displayName": "UnrealIRCd Configuration File Syntax Highlighter",
  "description": "Syntax highlighting for UnrealIRCd configuration files (*.conf).",
  "version": "1.0.0",
  "publisher": "Valware",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "unrealircd",
      "aliases": ["UnrealIRCd Config", "unrealircd"],
      "extensions": [".conf"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "unrealircd",
      "scopeName": "source.unrealircd",
      "path": "./syntaxes/unrealircd.tmLanguage.json"
    }]
  },
  "repository": "https://github.com/ValwareIRC/unrealircd-conf-shl"
}
