{
  "name": "unrealircd-conf-shl",
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "displayName": "UnrealIRCd Configuration File Syntax Highlighter (Unofficial)",
  "description": "Syntax highlighting for UnrealIRCd configuration files (*.conf).",
  "version": "1.1.0",
  "publisher": "Valware",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "unrealircd",
        "aliases": [
          "UnrealIRCd Config",
          "unrealircd"
        ],
        "extensions": [
          ".conf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "unrealircd",
        "scopeName": "source.unrealircd",
        "path": "./syntaxes/unrealircd.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "unrealircd",
        "path": "./unrealircd.conf-snippets"
      }
    ]
  },
  "repository": "https://github.com/ValwareIRC/unrealircd-conf-shl",
  "icon": "images/8283449.png",
  "devDependencies": {
    "@types/node": "^22.13.7",
    "typescript": "^5.7.3",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "cheerio": "^1.0.0"
  }
}
